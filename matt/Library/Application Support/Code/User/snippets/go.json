{
	"Wrap error": {
		"prefix": "re",
		"body": [
			"fmt.Errorf(\"%w $1\", err)"
		],
		"description": "Wrap a given error"
	},
	"Return error": {
		"prefix": "ife",
		"body": [
			"if err != nil {",
			"\t$1",
			"}"
		],
		"description": "Return the error"
	},
	"Fatal Error": {
		"prefix": "fa",
		"body": [
			"timber.Fatal(err, \"failed to $1\")",
		],
		"description": "log out a fatal error"
	}
}